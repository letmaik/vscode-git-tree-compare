{
	"name": "vscode-git-context",
	"displayName": "tbd",
	"description": "tbd",
	"version": "0.0.1",
	"publisher": "letmaik",
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:gitContext"
	],
	"main": "./out/src/extension",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "gitContext",
					"name": "Branch Compare"
				}
			]
		},
		"commands": [
			{
				"command": "gitContext.diffWithBase",
				"title": "Compare File with Base Branch"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "gitContext.diffWithBase",
					"when": "view == gitContext && viewItem == file"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.1.4",
		"vscode": "^1.0.0",
		"@types/node": "*"
	},
	"dependencies": {
		"iconv-lite": "0.4.15",
		"vscode-nls": "^2.0.1"
	}
}